                <fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-start-style="none" border-top-style="none" border-bottom-style="solid" border-bottom-width="0.5pt" border-bottom-color="black" border-end-style="solid" border-end-width="0.5pt" border-end-color="black" number-columns-spanned="2"><fo:block>/<fo:inline font-family="monospace" font-style="italic">account</fo:inline>/<fo:inline font-family="monospace" font-style="italic">container</fo:inline>/<fo:inline font-family="monospace" font-style="italic">object</fo:inline></fo:block></fo:table-cell>
